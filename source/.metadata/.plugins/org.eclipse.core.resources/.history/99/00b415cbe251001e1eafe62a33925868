package tut05;

public class ContEx {

	public static void main(String[] args) {
		//if
		//비교연산자 > < => true, false
		int num1 = 3;
		int num2 = 5;
		
//		if(조건) { //조건이 참
//			
//		}else { //조건의 반대
		if( num1 > num2 ) {
			System.out.println("로그아웃");
			System.out.println("시스탬을 종료");
		}else {
			System.out.println("로그인");
			System.out.println("시스탬을 종료");
	}
	}

}
