package thread;

import java.util.Scanner;

public class Ex{
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.println("입력>");
	int arr[] = new int[3];
	arr[0] = sc.nextInt();
	arr[1] = sc.nextInt();
	arr[2] = sc.nextInt();
	for(int i = 0; i < arr.length; i++) {
		if( arr[i] % 2 == 0 ) {
			System.out.println(arr[i]);
		}
		
	}
		
	
		
	}
	
}