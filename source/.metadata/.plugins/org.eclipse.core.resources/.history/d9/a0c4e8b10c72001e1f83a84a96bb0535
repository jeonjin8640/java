package exam;

public class Exam1 {

	public static void main(String[] args) {
		int i;
		for( i = 1; i <= 1000; i++) {
		if( (i%5) == 0 ) {
			System.out.println(i);
		}
		
		int i1;
		for( i1 = 1; i1 <= 1000; i++) {
			if( (i%3) == 0 ) {
				System.out.println(i1);
			}
		}
		int sum = i + i1;
		System.out.println( sum );
	}

}
