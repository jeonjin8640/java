package thread;

import java.util.Scanner;

public class Ex{
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.println("입력>");
	int[] num = new int[3];
	num[0] = sc.nextInt();
	num[1] = sc.nextInt();
	num[2] = sc.nextInt();
	for(int i = 0; i <num.length; i+=1) {
		if(num[i] % 2 == 0) {
			System.out.println(num[i]);
		}
		
	}
		
	}
	
}